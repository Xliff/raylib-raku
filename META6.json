{
  "auth": "zef:vushu",
  "authors": [
    "Dan Vu (vushu)"
  ],
  "build-depends": [],
  "depends": {
    "build": {
      "requires": [
        "Distribution::Builder::MakeFromJSON",
        {
          "from": "bin",
          "name": "raku"
        }
      ]
    },
    "runtime": {
      "requires": [
        {
          "from": "bin",
          "name": "raku"
        }
      ]
    }
  },
  "description": "Raylib bindings for raku, using grammar to parse raylib.h and auto generate the bindings",
  "license": "Artistic-2.0",
  "name": "Raylib::Bindings",
  "provides": {
    "Raylib::Bindings": "lib/Raylib/Bindings.rakumod"
  },
  "resources": [
    "libraries/raylib",
    "images/cat.png",
    "images/custom_alagard.png",
    "images/custom_mecha.png",
    "images/custom_jupiter_crash.png",
    "images/cyberpunk_street_background.png",
    "images/cyberpunk_street_midground.png",
    "images/cyberpunk_street_foreground.png",
    "images/parrots.png",
    "images/wabbit_alpha.png",
    "models/guy.iqm",
    "models/guytex.png",
    "models/guyanim.iqm",
    "models/guy.blend"
  ],
  "test-depends": [],
  "version": "0.0.15",
  "source-url": "https://github.com/vushu/raylib-raku.git",
  "builder": "Distribution::Builder::MakeFromJSON",
  "build": {
    "makefile-variables": {
      "rakuopts": {
        "run": [
          "raku",
          "configure.raku"
        ]
      }
    }
  }
}
